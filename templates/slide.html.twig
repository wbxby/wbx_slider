{#
/**
 * @file slide.html.twig
 * Default theme implementation to present Slide data.
 *
 * This template is used when viewing Slide pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_slide()
 *
 * @ingroup themeable
 */
#}
<a{{ attributes.addClass('slide', 'slide-' ~ slide.id()) }} href="{{ url }}">
  {% if content %}
    {{- content -}}
  {% endif %}
</a>
